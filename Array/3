// Print M, N bonacci numbers using sliding window

#include<iostream>
using namespace std;

int main(){
    int t;
    cin>>t;
    while(t--){
        int n, m;
        cin>>m>>n;
        int a[m];
        int index = 0, sum = 0;
        while(index < n){
            a[index] = 0;
            index++;
        }
        
        a[index] = 1;
        sum+= a[index];
        cout<<"index: "<<index<<endl;
        while(index < m){
            a[index] = sum - a[index - n];
            sum+= a[index];
            index++;
        }
        for(int i = 0; i < m; i++)
            cout<<a[i]<<" ";
        cout<<endl;
    }
    
    return 0;
}
